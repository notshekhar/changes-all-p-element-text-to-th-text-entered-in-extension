{
  "manifest_version": 2,
  "name": "slasho",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["index.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "slso144.png",
    "default_popup": "popup.html",
    "default_title": "slasho { codebeat }"
  }
}
